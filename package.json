{
  "name": "toktok",
  "displayName": "toktok",
  "description": "toktok syntax highlighting",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.21.0"
  },
  "publisher": "one7",
  "repository": {
    "type": "git",
    "url": "https://github.com/jannik4/vscode-toktok"
  },
  "contributes": {
    "languages": [
      {
        "id": "toktok",
        "extensions": [
          ".toktok",
          ".tt"
        ]
      }
    ],
    "grammars": [
      {
        "language": "toktok",
        "scopeName": "source.toktok",
        "path": "./syntaxes/toktok.tmGrammar.json"
      }
    ]
  }
}
